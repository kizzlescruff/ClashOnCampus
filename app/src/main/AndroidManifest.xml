<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wilki.clashoncampus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestListActivity" />
        <activity android:name=".QuestDescriptionActivity" />
        <activity android:name=".QuestMapActivity" />
        <activity android:name=".QuestInventoryActivity" />
        <activity android:name=".QuestFightActivity" />
        <activity android:name=".QuestCompleteActivity" />
        <activity android:name=".CreateQuestActivity" />
        <activity android:name=".CreateQuestMapActivity" />
        <activity android:name=".CreateQuestSetMonsterActivity" />
        <activity android:name=".CreateQuestFinishActivity"></activity>
    </application>

</manifest>